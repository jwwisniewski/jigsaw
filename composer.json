{
  "name": "jwwisniewski/jigsaw",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "fideloper/proxy": "^4.0",
    "jwwisniewski/jigsaw-news": "dev-master",
    "jwwisniewski/jigsaw-subpage": "dev-master",
    "laravel/framework": "^6.2",
    "laravel/tinker": "^2.0",
    "laravelcollective/errors": "^1.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0"
  },
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jwwisniewski/jigsaw-core"
    },
    {
      "type": "vcs",
      "url": "https://github.com/jwwisniewski/jigsaw-subpage"
    },
    {
      "type": "vcs",
      "url": "https://github.com/jwwisniewski/jigsaw-news"
    }
  ]
}
